  "main": "index.js",
  "scripts": {
	"prestart": "npm run updateAtjs && npm run minify",
    "start": "http-server . -p 80",
    "generateHtml": "node generateHtml.js",
    "updateAtjs": "rm -rf target/* && cp ../../atjs-library-v2/dist/at* target",
